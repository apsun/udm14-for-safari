{
  "manifest_version": 3,
  "name": "udm14 for Safari",
  "description": "Redirects Google to 'Web' search mode by default",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*.google.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "redirect-rules.json"
      }
    ]
  }
}
